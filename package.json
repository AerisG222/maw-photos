{
  "name": "maw-photos",
  "version": "1.7.5",
  "scripts": {
    "ng": "ng",
    "clean-unused-imports": "tslint --config tslint-imports.json --fix --project .",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "start": "ng serve --public-host photosdev.mikeandwan.us",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev_watch": "ng build --vendor-chunk=false --watch",
    "dev_build": "ng build --vendor-chunk=false && ng lint",
    "containerdev_build": "ng build --aot --configuration containerdev",
    "prod_build": "ng build --aot --prod",
    "containerize_dev": "buildah bud -f Containerfile.dev -t maw-photos-dev",
    "analyze": "source-map-explorer dist/main*.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.4",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "^11.0.4",
    "@angular/compiler": "^11.0.4",
    "@angular/core": "^11.0.4",
    "@angular/forms": "^11.0.4",
    "@angular/google-maps": "^11.0.3",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "^11.0.4",
    "@angular/platform-browser-dynamic": "^11.0.4",
    "@angular/router": "^11.0.4",
    "@ngrx/effects": "^10.1.1",
    "@ngrx/entity": "^10.1.1",
    "@ngrx/router-store": "^10.1.1",
    "@ngrx/store": "^10.1.1",
    "@ngrx/store-devtools": "^10.1.1",
    "@swimlane/ngx-charts": "^16.0.0",
    "angular-oauth2-oidc": "^10.0.3",
    "angular2-hotkeys": "^2.2.0",
    "file-saver": "^2.0.5",
    "hammerjs": "^2.0.8",
    "ngx-stars": "^1.4.3",
    "ngx-window-token": "^4.0.0",
    "numeral": "^2.0.6",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.4",
    "@angular/cli": "^11.0.4",
    "@angular/compiler-cli": "^11.0.4",
    "@angular/language-service": "^11.0.4",
    "@ngrx/schematics": "^10.1.1",
    "@types/file-saver": "^2.0.1",
    "@types/googlemaps": "^3.40.5",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.9",
    "@types/numeral": "0.0.28",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ngrx-tslint-rules": "^2.1.4",
    "protractor": "~7.0.0",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-etc": "^1.13.7",
    "typescript": "~4.0.5"
  }
}
