{
  "name": "maw-photos",
  "version": "1.7.2",
  "scripts": {
    "ng": "ng",
    "clean-unused-imports": "tslint --config tslint-imports.json --fix --project .",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "start": "ng serve --public-host photosdev.mikeandwan.us",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev_watch": "ng build --extract-css --vendor-chunk=false --watch",
    "dev_build": "ng build --extract-css --vendor-chunk=false --statsJson && ng lint",
    "containerdev_build": "ng build --aot --configuration containerdev --extract-css --vendor-chunk=false --stats-json",
    "prod_build": "ng build --aot --vendor-chunk=false --prod --statsJson",
    "bundle-report": "webpack-bundle-analyzer dist/stats-es2015.json",
    "containerize_dev": "buildah bud -f Containerfile.dev -t maw-photos-dev"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.9",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "^10.0.9",
    "@angular/compiler": "^10.0.9",
    "@angular/core": "^10.0.9",
    "@angular/forms": "^10.0.9",
    "@angular/google-maps": "^10.1.3",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "^10.0.9",
    "@angular/platform-browser-dynamic": "^10.0.9",
    "@angular/router": "^10.0.9",
    "@ngrx/effects": "^10.0.0",
    "@ngrx/entity": "^10.0.0",
    "@ngrx/router-store": "^10.0.0",
    "@ngrx/store": "^10.0.0",
    "@ngrx/store-devtools": "^10.0.0",
    "@swimlane/ngx-charts": "^14.0.0",
    "angular-oauth2-oidc": "^10.0.3",
    "angular2-hotkeys": "^2.2.0",
    "file-saver": "^2.0.2",
    "hammerjs": "^2.0.8",
    "ngx-stars": "^1.4.3",
    "ngx-window-token": "^3.0.0",
    "numeral": "^2.0.6",
    "rxjs": "^6.6.2",
    "tslib": "^2.0.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.6",
    "@angular/cli": "^10.0.6",
    "@angular/compiler-cli": "^10.0.9",
    "@angular/language-service": "^10.0.9",
    "@ngrx/schematics": "^10.0.0",
    "@types/file-saver": "^2.0.1",
    "@types/googlemaps": "^3.39.13",
    "@types/jasmine": "^3.5.12",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.12.54",
    "@types/numeral": "0.0.28",
    "codelyzer": "^5.2.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ngrx-tslint-rules": "^2.1.3",
    "protractor": "~7.0.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "tslint-etc": "^1.13.1",
    "typescript": "^3.9.7"
  }
}
