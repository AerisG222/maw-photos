{
  "name": "maw-photos",
  "version": "2.0.4",
  "scripts": {
    "ng": "ng",
    "report-unused-vars": "eslint --rule 'no-unused-vars: 0' --rule '@typescript-eslint/no-unused-vars: 2' . --ext .ts",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "start": "ng serve --public-host dev.photos.mikeandwan.us",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "eslint": "eslint",
    "e2e": "ng e2e",
    "dev_watch": "ng build --vendor-chunk=false --watch",
    "dev_build": "ng build --vendor-chunk=false && ng lint",
    "test_build": "ng build --aot --configuration test",
    "prod_build": "ng build --aot --configuration production",
    "analyze": "source-map-explorer dist/main*.js",
    "analyzeperf": "NG_BUILD_MINIFY=false ng serve --aot --configuration analyzeperf --public-host dev.photos.mikeandwan.us"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.10",
    "@angular/cdk": "^13.3.8",
    "@angular/common": "^13.3.10",
    "@angular/compiler": "^13.3.10",
    "@angular/core": "^13.3.10",
    "@angular/forms": "^13.3.10",
    "@angular/google-maps": "^13.3.8",
    "@angular/material": "^13.3.8",
    "@angular/platform-browser": "^13.3.10",
    "@angular/platform-browser-dynamic": "^13.3.10",
    "@angular/router": "^13.3.10",
    "@ngrx/effects": "^13.2.0",
    "@ngrx/entity": "^13.2.0",
    "@ngrx/router-store": "^13.2.0",
    "@ngrx/store": "^13.2.0",
    "@ngrx/store-devtools": "^13.2.0",
    "@swimlane/ngx-charts": "^20.1.0",
    "angular-oauth2-oidc": "^13.0.1",
    "angular2-hotkeys": "^13.1.0",
    "file-saver-es": "^2.0.5",
    "hammerjs": "^2.0.8",
    "ngx-bar-rating": "^3.0.0",
    "ngx-window-token": "^6.0.0",
    "numbro": "^2.3.6",
    "rxjs": "^7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.7",
    "@angular-eslint/builder": "^13.2.1",
    "@angular-eslint/eslint-plugin": "^13.2.1",
    "@angular-eslint/eslint-plugin-template": "^13.2.1",
    "@angular-eslint/schematics": "^13.2.1",
    "@angular-eslint/template-parser": "^13.2.1",
    "@angular/cli": "^13.3.7",
    "@angular/compiler-cli": "^13.3.10",
    "@angular/language-service": "^13.3.10",
    "@ngrx/schematics": "^13.2.0",
    "@types/d3": "^7.4.0",
    "@types/file-saver": "^2.0.5",
    "@types/google.maps": "^3.49.1",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "eslint-plugin-ngrx": "^2.1.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine-core": "~4.1.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.20",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.0.1",
    "karma-jasmine-html-reporter": "^2.0.0",
    "prettier": "^2.6.2",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.2",
    "ts-node": "^10.8.0",
    "typescript": "~4.6.2"
  }
}
