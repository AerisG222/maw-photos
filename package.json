{
  "name": "maw-photos",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "report-unused-vars": "eslint --rule 'no-unused-vars: 0' --rule '@typescript-eslint/no-unused-vars: 2' . --ext .ts",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "start": "ng serve --public-host photosdev.mikeandwan.us",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "eslint": "eslint",
    "e2e": "ng e2e",
    "dev_watch": "ng build --vendor-chunk=false --watch",
    "dev_build": "ng build --vendor-chunk=false && ng lint",
    "containerdev_build": "ng build --aot --configuration containerdev",
    "prod_build": "ng build --aot --prod",
    "containerize_dev": "buildah bud -f Containerfile.dev -t maw-photos-dev",
    "analyze": "source-map-explorer dist/main*.js",
    "analyzeperf": "NG_BUILD_MINIFY=false ng serve --aot --configuration analyzeperf --public-host photosdev.mikeandwan.us"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.1",
    "@angular/cdk": "^12.0.1",
    "@angular/common": "^12.0.1",
    "@angular/compiler": "^12.0.1",
    "@angular/core": "^12.0.1",
    "@angular/forms": "^12.0.1",
    "@angular/google-maps": "^12.0.1",
    "@angular/material": "^12.0.1",
    "@angular/platform-browser": "^12.0.1",
    "@angular/platform-browser-dynamic": "^12.0.1",
    "@angular/router": "^12.0.1",
    "@ngrx/effects": "^12.0.0",
    "@ngrx/entity": "^12.0.0",
    "@ngrx/router-store": "^12.0.0",
    "@ngrx/store": "^12.0.0",
    "@ngrx/store-devtools": "^12.0.0",
    "@swimlane/ngx-charts": "^18.0.1",
    "angular-oauth2-oidc": "^10.0.3",
    "angular2-hotkeys": "^2.2.0",
    "file-saver-es": "^2.0.5",
    "hammerjs": "^2.0.8",
    "ngx-bar-rating": "^2.0.0",
    "ngx-window-token": "^5.0.0",
    "numbro": "^2.3.2",
    "rxjs": "^6.6.6",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.1",
    "@angular-eslint/builder": "^12.0.0",
    "@angular-eslint/eslint-plugin": "^12.0.0",
    "@angular-eslint/eslint-plugin-template": "^12.0.0",
    "@angular-eslint/schematics": "^12.0.0",
    "@angular-eslint/template-parser": "^12.0.0",
    "@angular/cli": "^12.0.1",
    "@angular/compiler-cli": "^12.0.1",
    "@angular/language-service": "^12.0.1",
    "@ngrx/schematics": "^12.0.0",
    "@types/file-saver": "^2.0.2",
    "@types/googlemaps": "~3.43.3",
    "@types/jasmine": "~3.7.4",
    "@types/jasminewd2": "~2.0.9",
    "@types/node": "^15.6.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-ngrx": "^1.23.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "^3.4.0",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "ngrx-tslint-rules": "^2.1.4",
    "prettier": "^2.3.0",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.2",
    "ts-node": "^9.1.1",
    "typescript": "~4.2.4"
  }
}
